CC=gcc
CFLAGS=-Wall -O2 -funroll-loops -finline-functions -march=i586 -mcpu=i686 $(CYGWIN)
@HEADER@

.PHONY: all clean
SRC=fastutils.o.c
OBJ=fastutils.o
%.o: %.xs
	$(XSUBPP) -typemap "$(TYPEMAP)" $< > $@.c
	$(CC) -I. $(CFLAGS) $@.c -c $(PERLCFLAGS) -o $@ -I$(COREDIR)

all: $(OBJ)

clean:
	rm -f $(SRC) $(OBJ) Makefile.real
